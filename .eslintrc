{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "comma-dangle": 0,
        "block-scoped-var": 0,
        "padded-blocks": 0,
        "no-underscore-dangle": 0,
        "no-var": 0
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": []
}
